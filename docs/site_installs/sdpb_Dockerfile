FROM scratch
ADD rootfs.tar.xz /

RUN apt-get update && apt-get -y dist-upgrade && echo "Wed 23 Jun 2021 10:43:24 AM PDT"
RUN apt-get -y install openmpi-bin libgmp10 libgmpxx4ldbl libmpfr6 libboost-system1.67.0 libboost-filesystem1.67.0 libboost-program-options1.67.0 libboost-date-time1.67.0 libboost-iostreams1.67.0 libopenblas-base libmetis5 libtrilinos-teuchos12 libtrilinos-kokkos12 python3-lxml python3-mpmath libarchive13

ADD sdpb.tgz /usr/local
